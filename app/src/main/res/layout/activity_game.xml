<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".game">

    <TextView
        android:id="@+id/time"
        android:layout_width="86dp"
        android:layout_height="37dp"
        android:text="time"
        android:textColor="#F40303"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/count"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/count"
        android:layout_width="87dp"
        android:layout_height="37dp"
        android:text="count"
        android:textColor="#03A9F4"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/start"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout"
        app:layout_constraintGuide_begin="117dp"
        app:layout_constraintTop_toBottomOf="@+id/start" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="414dp"
        android:layout_height="0dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline5">

        <TableRow>


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon1"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon2"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon3"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />

        </TableRow>


        <TableRow>


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon4"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon5"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon6"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />

        </TableRow>


        <TableRow>


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon7"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon8"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/game_icon9"
                android:layout_width="40dp"
                android:layout_height="160dp"
                android:layout_marginStart="3dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="3dp"
                android:layout_weight="1"
                android:contentDescription="@string/StringValue"
                app:srcCompat="@drawable/teemo1" />

        </TableRow>


    </TableLayout>

    <ImageButton
        android:id="@+id/start"
        android:layout_width="143dp"
        android:layout_height="34dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="46dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/start" />

</androidx.constraintlayout.widget.ConstraintLayout>