        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_margin="10dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/background_color" />



        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_margin="10dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/background_color" />

    </LinearLayout>


</LinearLayout>

